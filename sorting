insertion sorting
main function
for(int i=1;i<n;i++){
	if(a[i]<a[i-1]){
		swap(a[i-1],a[i]) ;
	  int j=i-1;
	while(j!=0)
	{
		if(a[j-1]>a[j])
		{
				swap(a[j-1],a[j]) ;
		    j--; 
			}
		else break;
			
		}}
			
selection sorting

int min,p=0;
for(int i=0;i<n;i++){
	min=a[i];
	p=i;
		for(int k=i+1;k<n;k++){
			if(min>a[k]){
				min=a[k];
				p=k;
			}
		}
		swap(a[i],a[p]);
		
	
}
		
